 void val18(){
 	int x,y;
 	cin>>x>>y;
 	
 	if(x>y)
 		cout<<x<<" es el numero mas grande";
 	else
 		cout<<y<<" es el numero mas grande";
 	
 	
	}
	void val19(){
		int x,y,z,suma,promedio,producto,menor,mayor;
		cout<<"introduzca 3 numeros enteros : "<<endl;
		cin>>x>>y>>z;
		suma = x+y+z;
		promedio = suma/3;
		
		cout<<"suma : "<<suma<<endl;
		cout<<"promedio : "<<promedio<<endl;
		
		if((x>y)and(x>z))
			cout<<"el numero mayor es : "<<x<<endl;
		if((y>x)and(y>z))
			cout<<"el numero mayor es : "<<y<<endl;
		if((z>x)and(z>y))
			cout<<"el numero mayor es : "<<z<<endl;
			
		if((x<y)and(x<z))
			cout<<"el numero menor es : "<<x<<endl;
		if((y<x)and(y<z))
			cout<<"el numero menor es : "<<y<<endl;
		if((z<x)and(z<y))
			cout<<"el numero menor es : "<<z<<endl;
		
		
	}
	void val20(){
		int radio,diametro,circunferencia,area;
		double pi;
		pi=3.1415;
		
		cout<<"introduzca el radio del circulo : "<<endl;
		cin>>radio;
		diametro=2*radio;cout<<"el diametro es : "<<diametro<<endl;
		circunferencia=pi*diametro;cout<<"la circunferencia es : "<<circunferencia<<endl;
		area=pi*(radio*radio);cout<<"el area del circulo es : "<<area<<endl;
		
	}
	void val21(){
		
		cout<<" *********    ***      *       *    \n";
