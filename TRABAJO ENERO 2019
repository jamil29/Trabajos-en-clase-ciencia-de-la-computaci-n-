 void val23(){
 		
 	
 	int v1,v2,v3,v4,v5;
 	cout<<"introduzco 5 numeros : "<<endl;
 	cin>>v1>>v2>>v3>>v4>>v5;
 	
 	int valores[]{v1,v2,v3,v4,v5};
 		int mayor,menor;
				mayor=0;
				
				
 	for(int i=0;i<=4;i++){
				
				if(valores[i]>=mayor){
						mayor=valores[i];
						
				}
		}
		
 	cout<<"el valor mayor es : "<<mayor<<endl;
 	
 	menor = mayor;
 	
 	for(int i=0;i<=4;i++){
				
				if(valores[i]<=menor){
						menor=valores[i];
						
				}
		}
 	
 	cout<<"el valor menor es : "<<menor<<endl;
 	
	}
	
	
	void val24(){
		int valor;
