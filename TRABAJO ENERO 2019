#include <iostream>
using namespace std; 

 void val16(){
 	cout<<"dame dos numeros"<<endl;
 	int x,y;
 	cin>>x;
 	cin>>y;
 	
 	int suma,resta,multiplicacion,division;
 	suma = x+y;
 	resta = x-y;
 	multiplicacion = x*y;
 	division = x/y;
 	cout<<"la suma es :"<< suma<<endl;
 	cout<<"la resta es :"<< resta<<endl;
 	cout<<"la multiplicacion es :"<< multiplicacion<<endl;
 	cout<<"la division es :"<< division<<endl;
 }
 
