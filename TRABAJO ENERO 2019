	void val28(){
		int val;
		cout<<"vame un numero de 5 digitos "<<endl;
		cin>>val;
		
		int v1,v2,v3,v4,v5;
		v1=val/10000;
		val=val%10000;
		v2=val/1000;
		val=val%1000;
		v3=val/100;
		val=val%100;
		v4=val/10;
		val=val%10;
		v5=val/1;
		
		cout<<v1<<"   "<<v2<<"   "<<v3<<"   "<<v4<<"   "<<v5;
	}
	
	void val29(){
		cout<<" entero "<<" cuadrado "<<" cubo "<<endl;
		for(int i=0;i<=10;i++){
				cout<<i<<"         "<<i*i<<"         "<<i*i*i<<endl;
		}
	}
	
	
	
	void val30(){
		cout<<"BMI VALUES!!!!"<<endl;
		float peso,altura;
		cout<<"cual es su peso actual en kilogramos : "<<endl;
		cin>>peso;
		cout<<"cual es su altura actual en metros : "<<endl;
		cin>>altura;
		float bmi;
		bmi=peso/(altura*altura);
		cout<<"your bmi is "<<bmi<<endl ;
		cout<<"normal : 18.5 and 24.9 "<<endl;
		cout<<"exceso de peso :  25 and 29.9"<<endl;
		cout<<"sobrepeso : mas de 30 "<<endl;
		
	}
	
	void val31(){
		int gasolina,promedio,aparcamiento,peaje,ahorro;
		
	
		cout<<"cuanto es el costo de gasolina por tu zona(dolares)?"<<endl;
		cin>>gasolina;
		cout<<"promedio de millas por galon?"<<endl;
		cin>>promedio;
		cout<<"aproximado de tarifas de aparcamiento por dia ?"<<endl;
			cin>>aparcamiento;
		cout<<"costo total por peajes por dia ?"<<endl;
		cin>>peaje;
		
		ahorro=(promedio*gasolina)+aparcamiento+peaje;
		cout<<"el costo diario es de : "<<ahorro<<" dolares";
		
	}
